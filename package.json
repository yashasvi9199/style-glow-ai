{
  "name": "style-glow-ai",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://yashasvi9199.github.io/style-glow-ai",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "cap:add": "cap add android",
    "cap:sync": "cap sync",
    "cap:open": "cap open android",
    "android-release": "npm run build && npx cap sync && cd android && ./gradlew assembleRelease",
    "android-debug": "npm run build && npx cap sync && cd android && ./gradlew assembleDebug",
    "adb-install": "adb devices && cd android/app/build/outputs/apk/debug/ && adb install app-debug.apk"
  },
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@google/genai": "^1.30.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.344.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.18",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  }
}
